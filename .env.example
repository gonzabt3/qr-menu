# Database
DATABASE_URL=postgresql://user:password@localhost:5432/qr_menu_production

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3001,http://localhost:3000

# AWS S3 Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET_NAME=your_bucket_name

# MercadoPago
MERCADO_PAGO_ACCESS_TOKEN=your_mercado_pago_token

# Auth0
AUTH0_DOMAIN=your_auth0_domain
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Feedback
FEEDBACK_READ_SECRET=your_secret_for_reading_feedbacks

# AI Chat Feature (Optional)
# Set to 'true' to enable AI chat functionality
FEATURE_AI_CHAT_ENABLED=false

# AI Provider: 'deepseak' or 'openai' (default: deepseak)
AI_PROVIDER=deepseak

# DeepSeek API Key (required if AI_PROVIDER=deepseak)
DEEPSEAK_API_KEY=your_deepseak_api_key_here

# OpenAI API Key (required if AI_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here

# Enable detailed AI logging (default: false)
ENABLE_AI_CHAT_LOGS=false

# Redis URL for Sidekiq
REDIS_URL=redis://localhost:6379/0
